---
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import '../../src/styles/reset.css';
import '../../src/styles/footer.css';
import '../../src/styles/build.css';
const pageTitle = 'Hunt, Gather | Makers + Doers | Austin, TX';
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/hgc-astro/favicon.svg" />
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Oswald:wght@200..700&family=PT+Sans:wght@400;700&display=swap" rel="stylesheet">
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>{pageTitle}</title>
	</head>
	<body>
		<Header />
		<main>
			<slot />
		</main>
		<Footer/>
	</body>
</html>